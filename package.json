{
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "lint": "eslint *.js **/*.js",
    "test": "jest",
    "detox:android-build": "detox build --configuration android.emu.debug --stacktrace --info",
    "detox:android-test": "detox test --configuration android.emu.debug --cleanup --headless",
    "detox:android-build-release": "detox build --configuration android.emu.release",
    "detox:android-test-release": "detox test --configuration android.emu.release --cleanup --headless",
    "detox:clean-and-build-cache": "detox clean-framework-cache && detox build-framework-cache",
    "postinstall": "jetify"
  },
  "dependencies": {
    "expo": "~36.0.0",
    "expokit": "36.0.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-screens": "~2.0.0-alpha.12",
    "react-native-unimodules": "^0.7.0",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "~24.9.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "~8.0.0",
    "cross-fetch": "^3.0.4",
    "detox": "^14.9.1",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "jest-expo": "^36.0.1",
    "jest-fetch-mock": "^3.0.0",
    "jetifier": "~1.6.4",
    "metro-react-native-babel-preset": "~0.56.0",
    "react-test-renderer": "^16.12.0"
  },
  "jest": {
    "preset": "jest-expo",
    "automock": false,
    "testPathIgnorePatterns": [
      "setupJest.js",
      "e2e"
    ],
    "setupFiles": [
      "./__tests__/setupJest.js"
    ]
  },
  "private": true,
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus_S_API_24"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus_S_API_24"
        }
      }
    }
  }
}
